{
  "name": "api",
  "version": "1.0.0",
  "scripts": {
    "graphql:codegen": "graphql-codegen --config codegen.yml",
    "graphql:codegen:watch": "graphql-codegen --watch --config codegen.yml",
    "install-dependencies": "node scripts/recursiveCommand.js",
    "pre-build": "sh scripts/pre-build.sh",
    "lint": "eslint --ext .js,.ts lambdas",
    "format": "prettier --write lambdas",
    "start": ". env/.env.local && serverless offline --httpPort 5000",
    "deploy:dev": ". env/.env.dev && serverless deploy",
    "clean:dev": ". env/.env.dev && serverless remove"
  },
  "private": true,
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.7",
    "@graphql-codegen/introspection": "1.18.2",
    "@graphql-codegen/typescript": "1.23.0",
    "@graphql-codegen/typescript-document-nodes": "1.17.16",
    "@graphql-codegen/typescript-operations": "1.18.4",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "eslint": "^7.26.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "graphql": "^15.5.1",
    "prettier": "^2.2.1",
    "raw-loader": "^4.0.2",
    "rc": "^1.2.8",
    "serverless": "^2.64.0",
    "serverless-offline": "8.2.0",
    "serverless-webpack": "^5.4.2",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.2.4",
    "webpack": "^5.61.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.0",
    "webpack-merge": "^5.8.0"
  }
}
